def calculate(a,b, op="add"):
    if op=="add":
        return a+b
    elif op=="mult":
        return a*b
    elif op=="div":
        return a/b
    elif op=="mod":
        return a//b
    elif op=="sub":
        return a-b
    else:
        ("Unknown operation")
    
print(calculate(5,2))
print(calculate(5,2,"mult"))
print(calculate(5,2,"div"))
print(calculate(5,2,"mod"))
